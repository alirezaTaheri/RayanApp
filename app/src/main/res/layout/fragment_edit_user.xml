<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:layoutDirection="rtl">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginStart="10dp"        tools:layout_editor_absoluteX="16dp"        android:layout_marginTop="@dimen/materialize_toolbar"        tools:layout_editor_absoluteY="86dp"        android:orientation="vertical">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        tools:layout_editor_absoluteX="16dp"        tools:layout_editor_absoluteY="86dp"        android:orientation="horizontal">        <ImageView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="12dp"            app:srcCompat="@drawable/ic_email" />        <android.support.design.widget.TextInputLayout            android:id="@+id/nameInputLayout"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:textColorHint="@color/blue_dark_2">            <android.support.design.widget.TextInputEditText                android:id="@+id/name_EditUser_EditText"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="12dp"                android:layout_marginTop="15dp"                android:clickable="false"                android:gravity="right"                android:hint="@string/hint_edituser_name"                android:imeOptions="actionNext"                android:maxLength="15"                android:maxLines="1"                android:textSize="16sp" />        </android.support.design.widget.TextInputLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:focusable="true"        android:focusableInTouchMode="true"        tools:layout_editor_absoluteX="0dp"        tools:layout_editor_absoluteY="178dp">        <ImageView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="12dp"            app:srcCompat="@drawable/ic_email" />        <android.support.design.widget.TextInputLayout            android:id="@+id/phoneeInputLayout"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:textColorHint="@color/blue_dark_2"            app:passwordToggleContentDescription="Description"            app:passwordToggleEnabled="true"            app:passwordToggleTint="@color/baseColor2">            <android.support.design.widget.TextInputEditText                android:id="@+id/phone_EditUser_EditText"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="12dp"                android:layout_marginTop="15dp"                android:gravity="right"                android:clickable="false"                android:longClickable="false"                android:editable="false"                android:hint="@string/hint_edituser_phoneNumber"                android:imeOptions="actionDone"                android:inputType="phone"                android:maxLength="13"                android:maxLines="1"                android:textSize="16sp" />        </android.support.design.widget.TextInputLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:focusable="true"        android:focusableInTouchMode="true"        tools:layout_editor_absoluteX="0dp"        tools:layout_editor_absoluteY="260dp">        <ImageView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_margin="12dp"            app:srcCompat="@drawable/ic_email" />        <android.support.design.widget.TextInputLayout            android:id="@+id/emailInputLayout"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:textColorHint="@color/blue_dark_2"            app:passwordToggleContentDescription="Description"            app:passwordToggleEnabled="true"            app:passwordToggleTint="@color/baseColor2">            <android.support.design.widget.TextInputEditText                android:id="@+id/email_EditUser_EditText"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="12dp"                android:layout_marginTop="15dp"                android:clickable="false"                android:longClickable="false"                android:editable="false"                android:gravity="right"                android:hint="@string/hint_edituser_email"                android:imeOptions="actionDone"                android:inputType="textEmailAddress"                android:maxLines="1"                android:textSize="16sp" />        </android.support.design.widget.TextInputLayout>    </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:focusable="true"            android:layout_marginTop="20dp"            android:focusableInTouchMode="true"            tools:layout_editor_absoluteX="0dp"            tools:layout_editor_absoluteY="260dp">            <com.jaredrummler.materialspinner.MaterialSpinner                android:id="@+id/gender_EditUser_Spinner"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:hint="@string/gender_EditUser_Spinner_title"                android:textColorHint="#05ab9a"                android:focusable="true"                app:met_floatingLabel="normal" />        </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:focusable="true"        android:focusableInTouchMode="true"        tools:layout_editor_absoluteX="0dp"        tools:layout_editor_absoluteY="260dp">        <Button            android:id="@+id/edituser_submitbtn"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:focusable="true"            android:text="@string/edituser_submitbtn"            android:focusableInTouchMode="true"            android:background="@color/baseColor"            tools:layout_editor_absoluteX="0dp"            tools:layout_editor_absoluteY="260dp"/>    </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:focusable="true"            android:layout_marginTop="10dp"            android:focusableInTouchMode="true"            tools:layout_editor_absoluteX="0dp"            tools:layout_editor_absoluteY="260dp">            <Button                android:id="@+id/edituser_change_passwordbtn"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:focusable="true"                android:text="@string/edituser_change_passwordbtn"                android:focusableInTouchMode="true"                android:background="@color/baseColor"                tools:layout_editor_absoluteX="0dp"                tools:layout_editor_absoluteY="260dp"/>        </LinearLayout>    </LinearLayout></android.support.constraint.ConstraintLayout>